<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<Pane xmlns:fx="http://javafx.com/fxml/1" prefHeight="923.0" prefWidth="1181.0" xmlns="http://javafx.com/javafx/8"
      fx:controller="sample.Controller">
    <children>
        <TabPane layoutY="4.0" prefHeight="917.0" prefWidth="1900.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab text="General">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <Pane layoutY="1.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                                      prefHeight="887.0" prefWidth="1900.0">
                                    <children>
                                        <Label layoutX="18.0" layoutY="100.0" prefHeight="17.0" prefWidth="90.0"
                                               text="file extension"/>
                                        <Button fx:id="buttonStart" layoutX="250.0" layoutY="155.0"
                                                mnemonicParsing="false" onAction="#onStart" text="Search"/>
                                        <TextField fx:id="textFieldWord" layoutX="15.0" layoutY="126.0"
                                                   prefHeight="25.0" prefWidth="612.0"/>
                                        <TextField fx:id="textFieldSource" layoutX="15.0" layoutY="43.0"
                                                   prefHeight="25.0" prefWidth="328.0"/>
                                        <Label layoutX="18.0" layoutY="26.0" prefHeight="17.0" prefWidth="90.0"
                                               text="path"/>
                                        <Button fx:id="button" layoutX="143.0" layoutY="68.0" mnemonicParsing="false"
                                                onAction="#fileOpen" text="FileChooser"/>
                                        <TextArea fx:id="textAreaMessage" editable="false" layoutX="8.0" layoutY="185.0"
                                                  maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                                                  prefHeight="710.0" prefWidth="1900.0"/>
                                        <Button fx:id="buttonSetBasePath" layoutX="327.0" layoutY="11.0"
                                                mnemonicParsing="false" onAction="#onCreateBasePathSetting"
                                                prefHeight="25.0" prefWidth="132.0" text="set value by default"/>
                                        <TextField fx:id="textFieldBasePath" layoutX="63.0" layoutY="11.0"
                                                   prefHeight="25.0" prefWidth="258.0"/>
                                        <CheckBox fx:id="checkBoxJava" layoutX="100.0" layoutY="100.0"
                                                  mnemonicParsing="false" onAction="#setExtension" prefHeight="17.0"
                                                  prefWidth="57.0" text=" .java"/>
                                        <Button fx:id="buttonClearTextField" layoutX="309.0" layoutY="155.0"
                                                mnemonicParsing="false" onAction="#onClear" prefHeight="25.0"
                                                prefWidth="69.0" text="clear all"/>
                                        <Label fx:id="labelCount" layoutX="40.0" layoutY="72.0" prefHeight="23.0"
                                               prefWidth="90.0"/>
                                        <CheckBox fx:id="checkBoxLowCase" layoutX="85.0" layoutY="159.0"
                                                  mnemonicParsing="false" onAction="#setToLowerCase" prefHeight="17.0"
                                                  prefWidth="82.0" text="toLowCase"/>
                                    </children>
                                </Pane>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="Result">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="843.0" prefWidth="1166.0">
                            <children>
                                <TableView fx:id="tableView" layoutX="1.0" layoutY="-2.0"
                                           maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                                           prefHeight="891.0" prefWidth="1900.0"/>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</Pane>
